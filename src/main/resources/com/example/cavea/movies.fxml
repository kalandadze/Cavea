<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" onMouseClicked="#ToMoviePage" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #DEDEDE;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cavea.controllers.MoviesController">
   <children>
      <VBox prefHeight="200.0" prefWidth="210.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <children>
            <ImageView fx:id="image" fitHeight="297.0" fitWidth="200.0" pickOnBounds="true">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></ImageView>
            <Label fx:id="title" prefHeight="21.0" prefWidth="209.0" text="Label" wrapText="true">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
            <Button fx:id="rating" mnemonicParsing="false" prefHeight="16.0" prefWidth="29.0" style="-fx-background-color: #d0349c; -fx-text-fill: white;" text="rating" textAlignment="CENTER">
               <font>
                  <Font size="9.0" />
               </font>
               <padding>
                  <Insets bottom="-1.0" left="-1.0" right="-1.0" top="-1.0" />
               </padding>
            </Button>
         </children>
      </VBox>
   </children>
   <cursor>
      <Cursor fx:constant="HAND" />
   </cursor>
</AnchorPane>
