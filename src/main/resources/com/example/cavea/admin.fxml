<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="280.0" prefWidth="505.0" stylesheets="@../../../StyleSheets/buttonHover.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cavea.controllers.AdminController">
   <children>
      <HBox prefHeight="280.0" prefWidth="640.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#addMovie" prefHeight="291.0" prefWidth="300.0" style="-fx-background-color: #d0349c; -fx-text-fill: white;" text="კინოს დამატება">
               <font>
                  <Font size="29.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button mnemonicParsing="false" onAction="#deleteMovie" prefHeight="280.0" prefWidth="300.0" style="-fx-background-color: #d0349c; -fx-text-fill: white;" text="კინოების ნახვა">
               <font>
                  <Font size="29.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
